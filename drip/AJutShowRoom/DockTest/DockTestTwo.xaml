<UserControl x:Class="AJutShowRoom.DockTest.DockTestTwo"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:ajut="clr-namespace:AJut.UX.Controls;assembly=AJut.UX.WPF"
             xmlns:local="clr-namespace:AJutShowRoom.DockTest"
             x:Name="Self">
    <StackPanel Margin="10" VerticalAlignment="Center">
        <TextBlock Text="Number" HorizontalAlignment="Center" FontSize="30"/>
        <ajut:NumericEditor Value="{Binding ElementName=Self, Path=SaveData, Mode=TwoWay}"/>
    </StackPanel>
</UserControl>
